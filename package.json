{
  "name": "@cloud-weasel/traduora-client",
  "version": "0.0.7",
  "types": "build/index.d.ts",
  "main": "build/index.js",
  "description": "Traduora client for managing terms and translations",
  "repository": "https://github.com/cloud-weasel/traduora-client",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
